package recfun

object Worksheet {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  
  def minus(x: Int): Int = x-1                    //> minus: (x: Int)Int
  
  minus(2)                                        //> res0: Int = 1
  minus(1)                                        //> res1: Int = 0
  minus(0)                                        //> res2: Int = -1
  
}