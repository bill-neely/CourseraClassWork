package funsets

object assignment2_workssheet {
  type Set = Int => Boolean
  def singletonSet(elem: Int): Set = (x => x == elem)
  
  singletonSet(1)
  
  def filter(s: Set, p: Int => Boolean): Set = x => p(x)
                                                  //> filter: (s: Int => Boolean, p: Int => Boolean)Int => Boolean
  
}